<h1>New inventory</h1>

<% form_for(@inventory) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :item %><br />
    <%= f.text_field :item %>
  </p>
  <p>
    <%= f.label :in-stock %><br />
    <%= f.text_field :in-stock %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', inventories_path %>